{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "comment": "Triggering deployment after disabling 'Require Verified Commits'"
}
